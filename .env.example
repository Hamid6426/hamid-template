# Backend PORT
PORT=5000

# URL
DEV_FRONTEND_URL="http://localhost:5173"
DEV_BACKEND_URL="http://localhost:5000/api"

# URL
PROD_FRONTEND_URL="http://localhost:80"
PROD_BACKEND_URL="http://localhost:443/api"

# Development database URL
DEV_DATABASE_URL="postgres://myuser:mypassword@localhost:5432/mydb"

# Production database URL
PROD_DATABASE_URL=DATABASE_URL="postgres://<user>:<password>@<neon_host>:<port>/<dbname>?schema=public" 

# Vercel PostgreSQL (Neon) URL
NEON_API_URL="your_dev_neon_api_url"

# Database Settings
POSTGRES_USER=myuser
POSTGRES_PASSWORD=mypassword
POSTGRES_DB=mydb
POSTGRES_PORT=5432

# Docker Environments
NODE_PROD_ENV=production
NODE_DEV_ENV=development

